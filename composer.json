{
  "name": "opensmarty/rest-api-client",
  "description": "An elegant and smart Rest API Client with OAuth2 authentication support. Build for Laravel and Opensmarty Starter.",
  "keywords": [
    "rest",
    "api",
    "client",
    "opensmarty-starter",
    "oauth2",
    "laravel"
  ],
  "homepage": "https://github.com/opensmarty/rest-api-client",
  "license": "MIT",
  "authors": [
    {
      "name": "Nicolas Wan",
      "email": "opensmarty@163.com",
      "homepage": "https://opensmarty.github.io/"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "laravel/framework": "5.2.*|5.3.*|5.4.*|5.5.*",
    "guzzlehttp/guzzle": "~6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*"
  },
  "autoload": {
    "psr-4": {
      "Opensmarty\\Rest\\": "src/Opensmarty/Rest"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Opensmarty\\Rest\\Test\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "Opensmarty\\Rest\\RestClientServiceProvider"
      ]
    }
  }
}
